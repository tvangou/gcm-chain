ool.js","original":{"line":61,"column":16}},{"generated":{"line":125,"column":0},"source":"components/TransactionPool.js","original":{"line":62,"column":20}},{"generated":{"line":125,"column":8},"source":"components/TransactionPool.js","original":{"line":62,"column":20}},{"generated":{"line":125,"column":15},"source":"components/TransactionPool.js","original":{"line":62,"column":27}},{"generated":{"line":125,"column":17},"source":"components/TransactionPool.js","original":{"line":62,"column":28}},{"generated":{"line":125,"column":25},"source":"components/TransactionPool.js","original":{"line":61,"column":16}},{"generated":{"line":126,"column":0},"source":"components/TransactionPool.js","original":{"line":63,"column":20}},{"generated":{"line":126,"column":8},"source":"components/TransactionPool.js","original":{"line":63,"column":20}},{"generated":{"line":126,"column":15},"source":"components/TransactionPool.js","original":{"line":63,"column":27}},{"generated":{"line":126,"column":17},"source":"components/TransactionPool.js","original":{"line":63,"column":29}},{"name":"fetchMineTransactions","generated":{"line":126,"column":22},"source":"components/TransactionPool.js","original":{"line":63,"column":34}},{"generated":{"line":127,"column":0},"source":"components/TransactionPool.js","original":{"line":61,"column":16}},{"generated":{"line":127,"column":33},"source":"components/TransactionPool.js","original":{"line":47,"column":12}},{"generated":{"line":127,"column":34},"source":"components/TransactionPool.js","original":{"line":46,"column":8}},{"generated":{"line":128,"column":0},"source":"components/TransactionPool.js","original":{"line":69,"column":5}},{"name":"Component","generated":{"line":132,"column":2},"source":"components/TransactionPool.js","original":{"line":9,"column":30}},{"name":"TransactionPool","generated":{"line":134,"column":15},"source":"components/TransactionPool.js","original":{"line":72,"column":15}}],"sources":{"components/TransactionPool.js":"import React, { Component } from 'react';\nimport { Button } from 'react-bootstrap';\nimport Transaction from './Transaction';\nimport { Link } from 'react-router-dom';\nimport history from '../history';\n\nconst POLL_INTERVAL_MS = 10000;\n\nclass TransactionPool extends Component {\n    state = { transactionPoolMap: {} };\n\n    fetchTransactionPoolMap = () => {\n        fetch(`${document.location.origin}/api/transaction-pool-map`)\n        .then(response => response.json())\n        .then(json => this.setState({ transactionPoolMap: json }));\n    }\n\n    // Mine Transactions fetch to front end\n    fetchMineTransactions = () => {\n        fetch(`${document.location.origin}/api/mine-transactions`)\n        .then(response => {\n            if (response.status === 200) {\n                alert('success');\n                history.push('./blocks');\n            } else {\n                alert('The mine-transactions block requiest did not complete.');\n            }\n        });\n    }\n\n    componentDidMount() {\n        this.fetchTransactionPoolMap();\n\n        this.fetchPoolMapInterval = setInterval(\n            () => this.fetchTransactionPoolMap(),\n            POLL_INTERVAL_MS\n        )\n    }\n\n    componentWillUnmount() {\n        clearInterval(this.fetchPoolMapInterval);\n    }\n\n    // Returning transaction pool info, based on the Transaction data\n    render() {\n        return (\n            <div className='TransactionPool'>\n                <div><Link to='/'>Home</Link></div>\n                <h3>Transaction Pool</h3>\n                {\n                    Object.values(this.state.transactionPoolMap).map(transaction => {\n                        return (\n                            <div key={transaction.id}>\n                                <hr />\n                                <Transaction transaction={transaction} />\n                            </div>\n                        )\n                    })\n                }\n                <hr />\n                <Button \n                    bsstyle=\"danger\"\n                    onClick={this.fetchMineTransactions}\n                >\n                    Mine the Transactions\n                </Button>\n            </div>\n        )\n    }\n}\n\nexport default TransactionPool;"},"lineCount":null}},"error":null,"hash":"07956ccbccbd72b26771f3e94b661f91","cacheData":{"env":{}}}